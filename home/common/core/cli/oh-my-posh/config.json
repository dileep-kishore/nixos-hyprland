{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "rosewater": "#f5e0dc",
    "flamingo": "#f2cdcd",
    "pink": "#f5c2e7",
    "mauve": "#cba6f7",
    "red": "#f38ba8",
    "maroon": "#eba0ac",
    "peach": "#fab387",
    "yellow": "#f9e2af",
    "green": "#a6e3a1",
    "teal": "#94e2d5",
    "sky": "#89dceb",
    "sapphire": "#74c7ec",
    "blue": "#89b4fa",
    "lavender": "#b4befe",
    "text": "#cdd6f4",
    "subtext1": "#bac2de",
    "subtext0": "#a6adc8",
    "overlay2": "#9399b2",
    "overlay1": "#7f849c",
    "overlay0": "#6c7086",
    "surface2": "#585b70",
    "surface1": "#45475a",
    "surface0": "#313244",
    "base": "#1e1e2e",
    "mantle": "#181825",
    "crust": "#11111b"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "background": "transparent",
          "foreground": "p:mauve",
          "foreground_templates": ["{{ if ne .Code 0 }}p:red{{end}}"],
          "template": "┏"
        },
        {
          "type": "text",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:crust",
          "background": "p:mauve",
          "background_templates": ["{{ if ne .Code 0 }}p:red{{end}}"],
          "template": "  "
        },
        {
          "type": "status",
          "style": "accordion",
          "powerline_symbol": "",
          "foreground": "p:crust",
          "background": "p:blue",
          "background_templates": ["{{ if ne .Code 0 }}p:red{{end}}"],
          "template": " {{ .Code }}"
        },
        {
          "type": "text",
          "style": "accordion",
          "powerline_symbol": "",
          "foreground": "p:crust",
          "background": "p:yellow",
          "background_templates": ["{{ if gt .SHLVL 2 }}p:yellow{{end}}"],
          "template": "{{if gt .SHLVL 2}}  {{ .SHLVL }}{{end}}"
        },
        {
          "type": "root",
          "style": "plain",
          "foreground": "p:red",
          "background": "transparent",
          "template": " 󰿆 "
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "p:surface2",
          "background": "transparent",
          "template": "{{ if .SSHSession }} {{ .UserName }}@{{ .HostName }}{{ end }}"
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "p:yellow",
          "foreground_templates": ["{{ if gt .Code 0 }}p:red{{ end }}"],
          "properties": {
            "style": "round",
            "always_enabled": false,
            "threshold": 5000
          },
          "template": " <p:text><i>took</i></> 󰚭 {{ .FormattedMs }}"
        },
        {
          "type": "nix-shell",
          "style": "plain",
          "foreground": "p:blue",
          "template": " <p:text><i>in</i></>  nix-{{ .Type }}"
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "p:green",
          "template": " <p:text><i>via</i></> 󰎙 {{ .Full }}"
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:yellow",
          "template": " <p:text><i>via</i></>  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }} {{ if .Venv }}[{{ .Venv }}]{{ end }}{{ end }}"
        },
        {
          "type": "julia",
          "style": "plain",
          "foreground": "p:mauve",
          "template": " <p:text><i>via</i></>  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "p:sky",
          "template": " <p:text><i>via</i></>  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
        },
        {
          "type": "lua",
          "style": "plain",
          "foreground": "p:blue",
          "template": " <p:text><i>via</i></>  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
        },
        {
          "type": "haskell",
          "style": "plain",
          "foreground": "p:mauve",
          "template": " <p:text><i>via</i></>  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
        },
        {
          "type": "r",
          "style": "plain",
          "foreground": "p:blue",
          "template": " <p:text><i>via</i></>  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
        },
        {
          "type": "rust",
          "style": "plain",
          "foreground": "p:peach",
          "template": " <p:text><i>via</i></> 󱘗 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
        },
        {
          "type": "project",
          "style": "plain",
          "foreground": "p:teal",
          "template": " <p:text><i>is</i></> {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }} {{.Version}}{{ end }}{{ end }}"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "background": "transparent",
          "foreground": "p:mauve",
          "foreground_templates": ["{{ if ne .Code 0 }}p:red{{end}}"],
          "template": "┣"
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "p:blue",
          "properties": {
            "style": "agnoster_full"
          },
          "template": "{{ $segment := .Segments.Git }}{{ $icon := \" \" }}{{ if $segment.Dir }}{{ $dirL := splitList \"/\" $segment.Dir }} {{ $icon }} /{{ if $segment.IsWorkTree}}<b>{{ last $dirL }}</b>{{ else }}<b>{{ $segment.RepoName }}</b>{{ end }}{{if ne $segment.RepoName .Path}}{{ trimPrefix $segment.Dir .PWD }}{{ end }}{{ else }}  {{ path .Path .Location }}{{ end }}{{ if not .Writable }} \uf023{{ end }} "
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "p:green",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:peach{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if gt .Ahead 0 }}p:teal{{ end }}",
            "{{ if gt .Behind 0 }}p:maroon{{ end }}"
          ],
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "branch_icon": " ",
            "branch_identical_icon": " ",
            "branch_ahead_icon": "󰜹 ",
            "branch_behind_icon": "󰜰 ",
            "github_icon": " "
          },
          "template": "<p:text><i>on</i></> {{ .UpstreamIcon }}{{if .IsWorkTree}} {{end}}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} 󰤌 [{{ .Working.String }}]{{ end }}{{ if .Staging.Changed }} 󰄵 [{{ .Staging.String }}]{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "background": "transparent",
          "foreground": "p:mauve",
          "foreground_templates": ["{{ if ne .Code 0 }}p:red{{end}}"],
          "template": "┗ "
        }
      ]
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "p:lavender",
    "template": "{{ .PWD }}<p:mauve> ➜ </>",
    "newline": true
  },
  "final_space": false,
  "version": 2
}
